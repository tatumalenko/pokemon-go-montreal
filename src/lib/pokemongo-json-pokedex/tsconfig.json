{
	"compilerOptions": {
		"skipLibCheck": true,
		"module": "commonjs",
		"target": "es5",
		"noImplicitAny": false,
		"sourceMap": true,
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"baseUrl": ".",
		"paths": {
			"@core": ["src/core"],
			"@core/*": ["src/core/*"],
			"@components/*": ["src/components/*"],
			"@util": ["src/util"],
			"@util/*": ["src/util/*"],
			"@settings/app": ["src/app.settings"],
			"@settings/package": ["package.json"],
			"@data/*": ["src/data/*"],
			"@outcome": ["src/outcome"],
			"@outcome/*": ["src/outcome/*"],
			"@income": ["src/income"],
			"@income/*": ["src/income/*"]
		}
	}
}
